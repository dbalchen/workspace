#! /bin/sh

export UTLITIES_HOME=/home/dbalchen/workspace/usageUtlities

cd ${UTLITIES_HOME}/work

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | cut -f 1,3 | ../lib/sumBy.py > all_voice
../lib/makePicSeries.pl -o 01_all_voice -i  all_voice -s 1 -c "Voice Volume / Minutes" -t "All Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep N | cut -f 1,3 | ../lib/sumBy.py > all_home_voice
../lib/makePicSeries.pl -o 02_all_home_voice -i  all_home_voice -s 1 -c "Voice Home Volume / Minutes" -t "All Home Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep Y | cut -f 1,3 | ../lib/sumBy.py > all_roam_voice
../lib/makePicSeries.pl -o 03_all_roam_voice -i  all_roam_voice -s 1 -c "Voice Roam Volume / Minutes" -t "All Roam Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep POST| cut -f 1,3 | ../lib/sumBy.py > all_post_voice
../lib/makePicSeries.pl -o 04_all_post_voice -i  all_post_voice -s 1 -c "Post Voice Volume / Minutes" -t "All Post Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep N | grep POST| cut -f 1,3 | ../lib/sumBy.py > all_post_home_voice
../lib/makePicSeries.pl -o 05_all_post_home_voice -i  all_post_home_voice -s 1 -c "Voice Post Home Volume / Minutes" -t "All Post Home Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep Y | grep POST| cut -f 1,3 | ../lib/sumBy.py > all_post_roam_voice
../lib/makePicSeries.pl -o 06_all_post_roam_voice -i  all_post_roam_voice -s 1 -c "Voice Roam Volume / Minutes" -t "All Post Roam Voice for past 90 Days"



cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep PRE| cut -f 1,3 | ../lib/sumBy.py > all_pre_voice
../lib/makePicSeries.pl -o 07_all_pre_voice -i  all_pre_voice -s 1 -c "PRE Voice Volume / Minutes" -t "All PRE Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep N | grep PRE| cut -f 1,3 | ../lib/sumBy.py > all_pre_home_voice
../lib/makePicSeries.pl -o 08_all_pre_home_voice -i  all_pre_home_voice -s 1 -c "Voice PrePaid Home Volume / Minutes" -t "All Post Home Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep Y | grep PRE| cut -f 1,3 | ../lib/sumBy.py > all_pre_roam_voice
../lib/makePicSeries.pl -o 09_all_pre_roam_voice -i  all_pre_roam_voice -s 1 -c "Voice PrePaid Roam Volume / Minutes" -t "All Post Roam Voice for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep tas | cut -f 1,3 | ../lib/sumBy.py > all_volte_voice
../lib/makePicSeries.pl -o 10_all_volte_voice -i  all_volte_voice -s 1 -c "All VoLTE Volume / Minutes" -t "All VoLTE for past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep tas | grep N | cut -f 1,3 | ../lib/sumBy.py > all_volte_home_voice
../lib/makePicSeries.pl -o 11_all_volte_home_voice -i  all_volte_home_voice -s 1 -c "All VoLTE Home Volume / Minutes" -t "All VoLTE Home for the past 90 Days"

cat ../data/voice.dat | egrep -v 'VCDM|VLTE' | grep tas | grep Y | cut -f 1,3 | ../lib/sumBy.py > all_volte_roam_voice
../lib/makePicSeries.pl -o 12_all_volte_roam_voice -i  all_volte_roam_voice -s 1 -c "All VoLTE Roam Volume / Minutes" -t "All VoLTE Roam for the past 90 Days"